{
    "version": 2,
    "routes": [
      {
        "src": "/(.*).js",
        "dest": "/build/$1.js"
      },
      {
        "src": "/(.*).css",
        "dest": "/build/$1.css"
      },
      {
        "src": "/(.*).png",
        "dest": "/build/$1.png"
      },
      {
        "src": "/(.*).jpg",
        "dest": "/build/$1.jpg"
      },
      {
        "src": "/(.*).json",
        "dest": "/build/$1.json"
      },
      {
        "src": "/(.*).svg",
        "dest": "/build/$1.svg"
      },
      {
        "src": "/(.*).ico",
        "dest": "/build/$1.ico"
      }
    ]
  }
   